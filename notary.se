
# Notary
#
# API
#
#   # Set a value
#   call(NOTARY, ["set", key, value])
#
#   # Get a value
#   call(NOTARY, ["get", key])
#
code:
    command = msg.data[0]
    arg1 = msg.data[1]
    arg2 = msg.data[2]

    if command == "get":
        key = arg1
        value = storage[key]
        return(value)

    if command == "set":
        key = arg1
        value = arg2
        storage[key] = value
